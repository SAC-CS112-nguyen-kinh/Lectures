
import java.util.Scanner;

public class wk5Lab1{

	public static void main(String [] args){

	Scanner input = new Scanner(System.in);
	int thirsty;
		System.out.println("Thirsty?");
		thirsty = input.nextInt();
	
		if (thirsty == 1)
			System.out.println("Here is a can of coke");
		else
			System.out.println("No im not thirsty");


}



}
